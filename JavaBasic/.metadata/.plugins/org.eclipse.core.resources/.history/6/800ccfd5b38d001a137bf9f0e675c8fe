package reservationV1;

public class Member {
	
	String id;
	String pw;
	String name;
	String phonNumber;
	String address;
	String email;
	
	Member(String id, String pw, String name, String phoneNumber, String address, String email) {
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.phonNumber = phoneNumber;
		this.address = address;
		this.email = email;
	}
	
	public String toString() {
		return id + pw + name + phonNumber + address + email; 
	}
}
