package reservationV1;

public class Area {
	private String area;
	int[] areaCount;
	
	public String getArea() {
		return area;
	}
	public void setArea(String area) {
		this.area = area;
	}
	
	public void areaSearch() {
		System.out.print("지역을 검색해주세요 > ");
		String areaName = e_oop.ScanUtil.nextLine();
		area = areaName;
		areaCount++;
	}
}