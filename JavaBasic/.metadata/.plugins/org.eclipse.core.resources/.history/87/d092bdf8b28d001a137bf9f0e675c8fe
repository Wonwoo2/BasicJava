package reservationV1;

public class Member {
	
	private String id;
	private String pw;
	private String name;
	private String phonNumber;
	private String address;
	private String email;
	
	Member(String id, String pw, String name, String phoneNumber, String address, String email) {
		this.id = id;
		this.pw = pw;
		this.name = name;
		this.phonNumber = phoneNumber;
		this.address = address;
		this.email = email;
	}
	
	public String toString() {
		return id + pw + name + phonNumber + address + email; 
	}
}
