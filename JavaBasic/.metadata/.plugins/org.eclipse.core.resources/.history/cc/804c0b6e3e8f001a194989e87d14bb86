public class Store {

	public static void main(String[] args) {
		

	}

}

class Product {
	String name;
	int price;
	
	Product(String name, int price) {
		this.name = name;
		this.price = price;
	}
	
	void info() {
		System.out.println("------------------");
		System.out.println("상품명 : " + name);
	}
}
