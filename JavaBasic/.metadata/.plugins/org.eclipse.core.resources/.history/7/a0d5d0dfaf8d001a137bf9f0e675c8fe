package e_oop;

public class Reservation {
	
	public static void main(String[] args) {
		
	}
}


class Member {
	private String id;
	private String pw;
	
	public Member(String id, String pw) {
		this.id = id;
		this.pw = pw;
	}

	public String getId() {
		return id;
	}
	public String getPw() {
		return pw;
	}
}

class MemberInfo{
	String id;
	String pw;
	String name;
	String phonNumber;
	String address;
	String email;
}