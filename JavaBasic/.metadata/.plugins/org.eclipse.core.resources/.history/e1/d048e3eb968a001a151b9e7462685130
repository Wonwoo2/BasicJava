package e_oop;

public class TvControl {

	public static void main(String[] args) {
		TV tv = new TV();
		
		tv.powerOn();
		System.out.println("현재 채널은 " + tv.channel + "번 입니다.");
		tv.channelInput(49);
		
		tv.channelDown();
		tv.channelUp();
		
		tv.volumeUp();
		tv.volumeUp();
		
		tv.channelInput(300);
		tv.channelUp();
	}
}