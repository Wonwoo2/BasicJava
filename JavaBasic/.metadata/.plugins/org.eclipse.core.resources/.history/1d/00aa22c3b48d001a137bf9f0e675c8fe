package reservationV1;

public class MemberJoin {
	Member[] users = new Member[1000];
	int index = 0;
	void userRegister(String id, String pw, String name, String phoneNumber, String address, String email) {
		
		Member user = new Member();
		user.setId(id);
		user.setPw(pw);
		user.setName(name);
		user.setPhoneNumber(phoneNumber);
		user.setAddress(address);
		
		users[index++] = user;
	}
	
	void idCheck() {
		
	}
}